SHELL=/bin/bash
PATH=/home/ubuntu/dbling/src:/usr/bin
MAILTO=nobody@example.com
HOME=/home/ubuntu
XDG_RUNTIME_DIR=/run/user/1000

42 9,21 * * * { time /home/ubuntu/dbling/src/crx.py -up; } 2>&1 | mail -s "dbling: Result of list update" nobody@example.com
47 9,21 * * * { time /home/ubuntu/dbling/src/crx.py -sp 2> /home/ubuntu/dbling/src/errors.txt; cat /home/ubuntu/dbling/src/errors.txt; echo -e "\n"; cat /home/ubuntu/dbling/src/status.json; echo -e "\n"; tail /home/ubuntu/dbling/log/crx.log; } 2>&1 | mail -s "dbling: Result of centroid calculations" nobody@example.com

